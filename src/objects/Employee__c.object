    </validationRules>
    <validationRules>
        <fullName>Last_Working_Date_Should_Be_Smaller</fullName>
        <active>true</active>
        <errorConditionFormula>Date_of_Joining__c  &gt;  Last_Working_Date__c</errorConditionFormula>
        <errorMessage>Joining Date cannot be greater than Last Working Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_Size_Validation</fullName>
        <active>true</active>
        <description>The size of SSN value should be less than 10</description>
        <errorConditionFormula>LEN(TEXT( SSN__c ))  &gt;  10</errorConditionFormula>
        <errorMessage>Only 10 numbers can be input in SSN field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Salary_cannot_be_Process_for_Inactive</fullName>
        <active>true</active>
        <errorConditionFormula>Is_Active__c  = False</errorConditionFormula>
        <errorMessage>Salary Cannot be process for Inactive Employee</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Rule_on_Age</fullName>
        <active>true</active>
        <description>The Age should be between 21 - 30</description>
        <errorConditionFormula>Age__c &gt; 30  ||   Age__c  &lt; 21</errorConditionFormula>
        <errorMessage>Age Should be in between 21 years to 30 years</errorMessage>
    </validationRules>
